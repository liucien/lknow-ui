{
  "name": "lknow-ui",
  "version": "0.1.0",
  "description": "基于 vue-cli3 的 UI 组件库",
  "main": "lib/lknow-ui.umd.min.js",
  "keywords": "lknow-ui",
  "license": "MIT",
  "private": false,
  "scripts": {
